<!DOCTYPE html>
<html>
    <head>
        <title><%- @title %></title>
    </head>
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <body>
        
        <div class="container">


        <article>
        <div class="container">
            <h1><%- @title %></h1>
            <table class="table table-striped table-bordered table-auto datatable table-responsive" style="margin-left:auto;margin-right:auto;">
                <tr>
                    <th>IpAddress</th>
                    <th>Seconds</th>
                    <th>Name</th>
                    <th>Version</th>
                    <th>Platform</th>
                    <th>Taskname</th>
                    <th>UserName</th>
                    <th>Command</th>
                    <th>Comment</th>
                </tr>
                <% for m in @list : %>
                <tr>
                    <td><%- m.IpAddress %></td>
                    <td><%- m.Seconds %></td>
                    <td><%- m.OsName %></td>
                    <td><%- m.OsVersion %></td>
                    <td><%- m.Platform %></td>
                    <td><%- m.CurrentTaskname %></td>
                    <td><%- m.CurrentUserName %></td>
                    <td><%- m.Command %></td>
                    <td><%- m.Comment %></td>
                </tr>
                <% end %>
            </table>
        </div>
        </article> 
    </body>
</html